generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Duel {
  id        Int     @id @default(autoincrement())
  ia1       String
  ia2       String
  score1    Int
  score2    Int
  winner    String
  createdAt DateTime @default(now())
}